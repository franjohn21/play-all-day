<div class="container text-center">
  <h1> <%= @event.title %> </h1>
  <h4> <%= @event.location_name %> </h4>
  <%= @users.length %> users going:
  <ul>
    <% @users.length.times do |x| %>
    <div class="row">
      <div class="col-md-6 col-md-offset-3">
        <li>
          <p> <%= @users[x].username %> </p>
          <p> <%= @users[x].email %></p>
        </li>
      </div>
    </div>
    <% end %>
  </ul>
</div>